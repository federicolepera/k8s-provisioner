apiVersion: networking.k8s.io/v1
kind: Ingress
metadata:
  name: tekton-dashboard 
  namespace: tekton-pipelines 
  annotations:
    nginx.ingress.kubernetes.io/rewrite-target: /
spec:
  rules:
  - host: "{{ tekton_hostname }}"
    http:
      paths:
      - path: /
        pathType: Prefix
        backend:
          service:
            name: tekton-dashboard
            port:
              number: 9097